    "Normal startup": {
      "commandName": "Executable",
      "executablePath": "$(VVVVInstallPath)\\vvvv.exe",
      "commandLineArgs": "--package-repositories $(ThisPackageRepository);$(SolutionDir)\\..\\vl.mixer-packages --nuget-path $(SolutionDir)\\..\\nugets"
    },
    "Work on CoreLib": {
      "commandName": "Executable",
      "executablePath": "$(VVVVInstallPath)\\vvvv.exe",
