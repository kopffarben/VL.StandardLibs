    },
    "Work on Serialization": {
      "commandName": "Executable",
      "executablePath": "$(VVVVInstallPath)\\vvvv.exe",
      "commandLineArgs": "--package-repositories $(ThisPackageRepository);$(SolutionDir)\\..\\vl.mixer-packages --editable-packages VL.Serialization.* --nuget-path $(SolutionDir)\\..\\nugets"
    },
    "Work on ImGui": {
      "commandName": "Executable",
      "executablePath": "$(VVVVInstallPath)\\vvvv.exe",
